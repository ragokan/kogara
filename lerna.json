{
  "$schema": "https://json.schemastore.org/lerna.json",
  "packages": [
    "packages/*"
  ],
  "workspaces": [
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "0.0.15",
  "npmClient": "pnpm",
  "command": {
    "version": {
      "exact": true
    },
    "publish": {
      "npmClient": "pnpm",
      "allowBranch": [
        "master"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  }
}
