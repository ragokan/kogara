{
  "name": "@kogara/monorepo",
  "version": "0.0.0",
  "description": "Monorepo of kogara packages",
  "homepage": "https://github.com/ragokan/kogara",
  "author": "ragokan",
  "license": "MIT",
  "private": false,
  "workspaces": [
    "docs",
    "packages/*",
    "example"
  ],
  "scripts": {
    "build": "pnpm format && turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --loglevel 'error' --write \"**/*.{ts,vue,md,json}\"",
    "prepublish": "pnpm build",
    "publish": "lerna publish --force-publish --yes"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@swc/core": "^1.2.145",
    "@types/node": "^17.0.21",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "tsup": "^5.11.13",
    "turbo": "latest",
    "typescript": "^4.5.5",
    "vue": "^3.2.31"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.32.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/kogara.git"
  }
}
