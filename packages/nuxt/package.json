{
  "name": "@kogara/nuxt",
  "version": "0.15.0",
  "private": false,
  "description": "Kogara Nuxt module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "build": "yarn run lint && yarn run prepack && changelogen --release && yarn publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.1",
    "@nuxt/test-utils": "^3.7.1",
    "@types/node": "^20.6.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.49.0",
    "nuxt": "^3.7.1",
    "vitest": "^0.34.4"
  },
  "overrides": {
    "wide-align": "git://github.com/jimmywarting/wide-align.git#es6-style"
  }
}
