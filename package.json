{
  "name": "@kogara/monorepo",
  "version": "0.0.0",
  "description": "Monorepo of kogara packages",
  "homepage": "https://github.com/ragokan/kogara",
  "author": "ragokan",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "example",
    "local"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "docs:dev": "turbo run docs:dev --parallel",
    "example:dev": "turbo run example:dev --parallel",
    "nuxt:dev": "turbo run nuxt:dev --parallel",
    "test": "vitest --config vitest.config.ts",
    "test-watch": "vitest --config vitest.config.ts --watch",
    "prepublish": "yarn build",
    "publish": "lerna publish --force-publish --yes",
    "update-all": "ncu -u && turbo run update-all --parallel"
  },
  "devDependencies": {
    "@babel/types": "^7.17.10",
    "@swc/core": "^1.2.175",
    "@types/node": "^17.0.31",
    "@types/ws": "^8.5.3",
    "lerna": "^5.0.0",
    "npm-check-updates": "^14.0.0",
    "prettier": "^2.6.2",
    "tsup": "^6.0.0",
    "turbo": "latest",
    "typescript": "^4.6.4",
    "vitest": "^0.15.0",
    "vue": "^3.2.33"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/kogara.git"
  }
}
