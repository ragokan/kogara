{
  "name": "@kogara/di",
  "version": "0.4.6",
  "description": "Simple dependency injection everywhere",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --sourcemap --dts --format esm,cjs,iife --legacy-output",
    "dev": "tsup src/index.ts --sourcemap --dts --format esm,cjs,iife --legacy-output --watch --onSuccess 'node dist/index.js'",
    "deps-update-all": "ncu -u"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "dependency injection",
    "state",
    "kogara"
  ],
  "homepage": "https://github.com/ragokan/kogara",
  "author": "ragokan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/kogara.git",
    "directory": "packages/di"
  },
  "gitHead": "d6a70d27f09db28fdbcec1a4a83e7b820c4ee9a4"
}
