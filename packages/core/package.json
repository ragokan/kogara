{
  "name": "@kogara/core",
  "version": "0.0.10",
  "description": "Core library of kogara",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsup src/index.ts --minify --clean --sourcemap --dts --format esm,cjs,iife --legacy-output",
    "build-watch": "tsup src/index.ts --minify --clean --sourcemap --dts --format esm,cjs,iife --legacy-output --watch --onSuccess 'node dist/index.js'",
    "test": "vitest --config ../../vitest.config.ts",
    "test-watch": "vitest --config ../../vitest.config.ts --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "keywords": [
    "vue",
    "state",
    "kogara"
  ],
  "homepage": "https://github.com/ragokan/kogara",
  "author": "ragokan",
  "license": "MIT",
  "devDependencies": {
    "@vue/devtools-api": "6.0.12"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ragokan/kogara.git",
    "directory": "packages/core"
  },
  "gitHead": "4c407295e080891c37a16bc51949d25ad51aff8a"
}
