{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "test": {
      "cache": true,
      "outputs": []
    },
    "coverage": {
      "cache": true,
      "outputs": []
    },
    "publish": {
      "dependsOn": ["lint", "build"],
      "cache": false
    }
  }
}
