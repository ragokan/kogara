{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true
    },
    "dev": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "docs:dev": {
      "cache": false
    },
    "example:dev": {
      "cache": false
    },
    "nuxt:dev": {
      "cache": false
    },
    "deps-update-all": {
      "cache": false
    }
  }
}
